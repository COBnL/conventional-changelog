{
  "extends": [
    "canonical"
  ],
  "root": true,
  "rules": {
    "arrow-body-style": [
      "error",
      "as-needed"
    ],
    "filenames/match-regex": [
      2,
      "^[A-Z]?[a-z]+(?:[A-Z][a-z]+)*(\\.[a-z]+)*$",
      false
    ],
    "no-var": "off",
    "vars-on-top": "off",
    "no-restricted-syntax": "off",
    "sort-keys": "off",
    "object-shorthand": "off",
    "prefer-arrow-callback": "off",
    "import/no-commonjs": "off",
    "import/unambiguous": "off",
    "promise/prefer-await-to-then": "off"
  }
}